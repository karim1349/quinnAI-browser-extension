{
  "name": "quinnai-browser-extension",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "deps": "npm install",
    "stage": "mkdir dist || true",
    "bundle": "esbuild src/gmailJsLoader.js src/extension.js --outdir=dist/ --bundle --sourcemap --target=es6 --loader:.js=jsx",
    "devbuild": "cp ../gmail.js/src/gmail.js node_modules/gmail-js/src/ && npm run build",
    "build": "npm run stage && npm run bundle"
  },
  "dependencies": {
    "gmail-js": "^1.1.7",
    "jquery": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.16.0"
  }
}
